mixin partners()
  section.partners
    .partners__inner
      h2.partners__heading Партнёры
      .partners__slider
        ul.partners__list
          mixin partners__item(partnersItem_number)
            li.partners__item
              +brand-logo(partnersItem_number)
          - let i = 1;
          while i <= 9
            +partners__item(i)
            - i++;
